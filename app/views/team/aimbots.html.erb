<% team = Team.find_by_name("Aimbots") %>
<div class="row align-items-center justify-content-center top-cover-splash-aimbots">
    <div class="col-sm-2">
        <img style="max-height: 250px;" src="<%= asset_path 'season_three_teams/discord/aimbots_too.png' %>">
    </div>
    <div class="col-sm-2">
        <h1><%= team.name %></h1>
    </div>
    <div class="col-sm-2">
        <h6>Division</h6>
        <%= link_to tier_discord_path, class: 'top-cover-link-black' do %><h4><%= team.tier.upcase %></h4> <% end %>
        <br/>
        <h6>SR</h6>
        <h4><%= team.sr %></h4>
    </div>
    <div class="col-sm-2">
        <h6>GM</h6>
        <h4><%= team.gm %></h4>
        <br/>
        <h6>Coach</h6>
        <h4><%= team.coach %></h4>
    </div>
    <div class="col-sm-2">
        <h6>Twitter</h6>
        <h4><a class="top-cover-link-black" href="https://twitter.com/<%= team.twitterlink %>"><%= team.twittername %></a></h4>
        <br/>
        <h6>Discord</h6>
        <h4><a class="top-cover-link-black" href="<%= team.discordlink %>"><%= team.discordname %></a></h4>
    </div>
    <div class="col-sm-1"></div>
</div>
<br/>
<h2>Roster:</h2>
<% team.player.each do |x| %>
    <div class="row">
        <div class="col-sm-4"></div>
        <div class="col-sm-4"><h4 style="color: red"><%= x.name %></h4></div>
        <div class="col-sm-4"></div>
      
    </div>
<% end %>